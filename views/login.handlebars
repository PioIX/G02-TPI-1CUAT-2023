
<div><form action="/login" method="POST" class="container-sm">
    <div class="mb-3 form-group">
        <center><h2>Iniciar sesion</h2></center>
    </div>
    <div class="mb-3 form-group">
        <label for="usuarioId" class="form-label">Usuario</label>
        <center><input type="text" class="form-control" id="usuarioId" name="usuario" value="" placeholder="Ingrese un nombre de usuario"></center>
    </div>
    <div class="mb-3 form-group">
        <label for="passwordId" class="form-label">Contraseña</label>
        <center><input type="password" class="form-control" id="passwordId" name="contraseña" placeholder="Contraseña"></center>
    </div>
    <div class="mb-3 form-check">
        <center><input type="submit" class="botonlogin" value="Ingresar"></center>
    </div>
</form>
<form action="/register" method="GET" class="container-sm">
    <div class="mb-3 form-check">
        <center><input type="submit" class="botonregistro" value="Registrarse"></center>
    </div>
</form></div>

